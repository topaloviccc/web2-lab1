{
	"compilerOptions": {
		"target": "ES2020",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"esModuleInterop": true,
		"outDir": "./dist",
		"rootDir": "./src",
		"declaration": true,
		"strictNullChecks": true,
		"noImplicitAny": true,
		"noEmitOnError": true,
		"skipLibCheck": true // ako nes faila ovo je razlog
	}
}
